    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "localtunnel": "1.8.1",
    "npm-run-all": "^4.0.2",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "webpack": "1.13.2"
  }
